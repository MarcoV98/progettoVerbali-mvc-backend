@model List<TotaleVerbaliVM>

<h2>Totale Verbali per Trasgressore</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID Anagrafica</th>
            <th>Cognome</th>
            <th>Nome</th>
            <th>Totale Verbali</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.IDAnagrafica</td>
                <td>@item.Cognome</td>
                <td>@item.Nome</td>
                <td>@item.TotaleVerbali</td>
            </tr>
        }
    </tbody>
</table>
