@model List<TotalePuntiVM>

<h2>Totale Punti per Trasgressore</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID Anagrafica</th>
            <th>Cognome</th>
            <th>Nome</th>
            <th>Totale Punti</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.IDAnagrafica</td>
                <td>@item.Cognome</td>
                <td>@item.Nome</td>
                <td>@item.TotalePunti</td>
            </tr>
        }
    </tbody>
</table>
